// Example program
#include <iostream>
#include <string>
using namespace std; 
class Fecha{
   int anio; 
   int mes;
   int dia;

public:
    void set_values (int,int,int);
    int getdia()
        {
            return dia;
            }
};

void Fecha::set_values (int x, int y, int z) {
  anio = x;
  mes = y;
  dia = z;
}

int main () {
    
  Fecha f1;
    Fecha f2;
  f1.set_values (2017,8,11);
  f2.set_values (2016,5,29);
  cout<<f1.getdia();
  cout<<f2.getdia();
  return 0;
  
}
